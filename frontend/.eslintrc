{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
    "max-len": 0,
    "no-nested-ternary": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "react/prop-types": 0,
    "linebreak-style": ["warn", "windows"],
    "jsx-a11y/click-events-have-key-events" : 0
  }
}